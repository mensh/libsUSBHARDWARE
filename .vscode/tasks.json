{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build project",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "type": "shell",
            "command": "C:/Users/User/AppData/Roaming/GNUMCUEclipse/BuildTools/bin/make.exe",
            "args": [
                "GCC_PATH=C:/Users/User/AppData/Roaming/GNUMCUEclipse/ARMEmbeddedGCC/bin",
                "-j6"
            ],
            "problemMatcher": {
                "pattern": {
                    "regexp": "^(.*):(\\d+):(\\d+):\\s+(warning|error):\\s+(.*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5
                }
            },
            "presentation": {
                "focus": true
            }
        },
        {
            "label": "Compile current file",
            "type": "shell",
            "command": "C:/Users/User/AppData/Roaming/GNUMCUEclipse/ARMEmbeddedGCC/bin/arm-none-eabi-gcc.exe",
            "args": [
                "-c",
                "-DUSE_HAL_DRIVER",
                "-DSTM32F215xx",
                "-DUSE_HAL_DRIVER",
                "-DSTM32F215xx",
                "-IInc",
                "-IDrivers/STM32F2xx_HAL_Driver/Inc",
                "-IDrivers/STM32F2xx_HAL_Driver/Inc/Legacy",
                "-IMiddlewares/Third_Party/FreeRTOS/Source/include",
                "-IMiddlewares/Third_Party/FreeRTOS/Source/CMSIS_RTOS",
                "-IMiddlewares/Third_Party/FreeRTOS/Source/portable/GCC/ARM_CM3",
                "-IMiddlewares/ST/STM32_USB_Device_Library/Core/Inc",
                "-IMiddlewares/ST/STM32_USB_Device_Library/Class/CDC/Inc",
                "-IDrivers/CMSIS/Device/ST/STM32F2xx/Include",
                "-IDrivers/CMSIS/Include",
                "-IDrivers/CMSIS/Include",
                "-mcpu=cortex-m3",
                "-mthumb",
                "-DUSE_HAL_DRIVER",
                "-DSTM32F215xx",
                "-DUSE_HAL_DRIVER",
                "-DSTM32F215xx",
                "-IInc",
                "-IDrivers/STM32F2xx_HAL_Driver/Inc",
                "-IDrivers/STM32F2xx_HAL_Driver/Inc/Legacy",
                "-IMiddlewares/Third_Party/FreeRTOS/Source/include",
                "-IMiddlewares/Third_Party/FreeRTOS/Source/CMSIS_RTOS",
                "-IMiddlewares/Third_Party/FreeRTOS/Source/portable/GCC/ARM_CM3",
                "-IMiddlewares/ST/STM32_USB_Device_Library/Core/Inc",
                "-IMiddlewares/ST/STM32_USB_Device_Library/Class/CDC/Inc",
                "-IDrivers/CMSIS/Device/ST/STM32F2xx/Include",
                "-IDrivers/CMSIS/Include",
                "-IDrivers/CMSIS/Include",
                "-Og",
                "-Wall",
                "-fdata-sections",
                "-ffunction-sections",
                "-g",
                "-gdwarf-2",
                "-MMD",
                "-MP",
                "-MF'build/${fileBasenameNoExtension}.d'",
                "'${relativeFile}'",
                "-o",
                "'build/${fileBasenameNoExtension}.o'"
            ],
            "problemMatcher": {
                "pattern": {
                    "regexp": "^(.*):(\\d+):(\\d+):\\s+(warning|error):\\s+(.*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5
                }
            },
            "presentation": {
                "focus": true
            }
        },
        {
            "label": "Delete build folder",
            "type": "shell",
            "command": "C:/Users/User/AppData/Roaming/GNUMCUEclipse/BuildTools/bin/make.exe",
            "args": [
                "clean"
            ],
            "problemMatcher": [],
            "presentation": {
                "focus": false
            }
        },
        {
            "label": "CPU: Build, Download and run",
            "type": "shell",
            "command": "C:/Users/User/AppData/Roaming/GNUMCUEclipse/OpenOCD/bin/openocd.exe",
            "args": [
                "-f",
                "C:/Users/User/AppData/Roaming/GNUMCUEclipse/OpenOCD/scripts/interface/stlink-v2.cfg",
                "-f",
                ".vscode/stm32f2x.cfg",
                "-c",
                "program build/TestUSB.elf verify reset exit"
            ],
            "problemMatcher": [],
            "dependsOn": "Build project"
        },
        {
            "label": "CPU: Download and run",
            "type": "shell",
            "command": "C:/Users/User/AppData/Roaming/GNUMCUEclipse/OpenOCD/bin/openocd.exe",
            "args": [
                "-f",
                "C:/Users/User/AppData/Roaming/GNUMCUEclipse/OpenOCD/scripts/interface/stlink-v2.cfg",
                "-f",
                ".vscode/stm32f2x.cfg",
                "-c",
                "program build/TestUSB.elf verify reset exit"
            ],
            "problemMatcher": []
        },
        {
            "label": "CPU: Reset and run",
            "type": "shell",
            "command": "C:/Users/User/AppData/Roaming/GNUMCUEclipse/OpenOCD/bin/openocd.exe",
            "args": [
                "-f",
                "C:/Users/User/AppData/Roaming/GNUMCUEclipse/OpenOCD/scripts/interface/stlink-v2.cfg",
                "-f",
                ".vscode/stm32f2x.cfg",
                "-c init",
                "-c reset",
                "-c exit"
            ],
            "problemMatcher": []
        },
        {
            "label": "CPU: Halt",
            "type": "shell",
            "command": "C:/Users/User/AppData/Roaming/GNUMCUEclipse/OpenOCD/bin/openocd.exe",
            "args": [
                "-f",
                "C:/Users/User/AppData/Roaming/GNUMCUEclipse/OpenOCD/scripts/interface/stlink-v2.cfg",
                "-f",
                ".vscode/stm32f2x.cfg",
                "-c init",
                "-c halt",
                "-c exit"
            ],
            "problemMatcher": []
        },
        {
            "label": "CPU: Run",
            "type": "shell",
            "command": "C:/Users/User/AppData/Roaming/GNUMCUEclipse/OpenOCD/bin/openocd.exe",
            "args": [
                "-f",
                "C:/Users/User/AppData/Roaming/GNUMCUEclipse/OpenOCD/scripts/interface/stlink-v2.cfg",
                "-f",
                ".vscode/stm32f2x.cfg",
                "-c init",
                "-c resume",
                "-c exit"
            ],
            "problemMatcher": []
        },
        {
            "label": "Run Python file",
            "type": "shell",
            "command": "C:/Users/User/AppData/Local/Microsoft/WindowsApps",
            "args": [
                "${file}"
            ],
            "presentation": {
                "focus": true
            },
            "problemMatcher": []
        },
        {
            "label": "Open CubeMX project",
            "type": "shell",
            "command": "start",
            "args": [
                "TestUSB.ioc"
            ],
            "presentation": {
                "focus": false
            },
            "problemMatcher": []
        },
        {
            "label": "Update workspace",
            "type": "shell",
            "command": "C:/Users/User/AppData/Local/Microsoft/WindowsApps",
            "args": [
                "${workspaceFolder}/ideScripts/update.py"
            ],
            "presentation": {
                "focus": true
            },
            "problemMatcher": []
        }
    ]
}